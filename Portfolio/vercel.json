{
  "version": 2,
  "builds": [
      {
          "src": "*.php",
          "use": "@vercel/php"
      },
      {
          "src": "*.html",
          "use": "@vercel/static-build"
      }
  ],
  "routes": [
      {
          "src": "/contact-form",
          "dest": "/contact-form.php"
      },
      {
          "src": "/newsletter",
          "dest": "/newsletter.php"
      },
      {
          "src": "/(.*)",
          "dest": "/index.html"
      }
  ]
}